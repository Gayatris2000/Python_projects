   "source": [
    "words = [\"apple\", \"banana\", \"cherry\", \"blueberry\"]\n",
    "longest_word = words[0]\n",
    "for word in words:\n",
    "    if len(word) > len(longest_word):\n",
    "        longest_word = word\n",
    "\n",
    "print(f\"{longest_word} is the longest word with {len(longest_word)} characters.\")\n",
    "\n"
